{
	"name": "docs-no-localization",
	"displayName": "Docs No Localization",
	"description": "Identify Content to NOT be localized",
	"publisher": "cillroy",
	"author": "cillroy",
	"icon": "images/docs-dot-com.png",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.31.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.docs-no-localization"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.docs-no-localization",
				"title": "No Localization",
				"category": "Docs"
			}
		],
		"keybindings": [
			{
				"command": "extension.docs-no-localization",
				"key": "ctrl+d ctrl+n",
				"mac": "ctrl+d ctrl+n"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"license": "SEE LICENSE IN LICENSE",
	"repository": {
		"type": "git",
		"url": "https://github.com/cillroy/docs-no-localization.git"
	}
}
